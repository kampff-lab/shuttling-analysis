                <q3:Script>steps = None

def load():
    global steps
    steps = None

def process(input):
    global steps
    if steps is None:
        steps = [open("step%s.csv" % (f),'a') for f in range(input.Count)]

    for i in range(input.Count):
        steps[i].write("%s " % (input[i].Activity.Val0))

def unload():
    global steps
    for file in steps:
        file.write("\n")
        file.close()</q3:Script>
